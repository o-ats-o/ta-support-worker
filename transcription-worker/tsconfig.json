{
	"compilerOptions": {
	  "target": "ES2022",
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "lib": ["ES2022"],
	  "allowJs": true,
	  "checkJs": false,
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "strict": true,
	  "skipLibCheck": true,
	  "jsx": "react-jsx",
	  "jsxImportSource": "hono/jsx",
	  "types": ["@cloudflare/workers-types"]
	},
	"include": ["src"]
}